    {
      "key": "token_desc_arrow",
      "value": "When a Fourzy enters a space with an Arrow, it is redirected in the direction of the Arrow."
    },
    {
      "key": "token_desc_blocker",
      "value": "Blocker. Fourzies cannot enter or move through this space."
    },
    {
      "key": "token_desc_circle_bomb",
      "value": "When a Fourzy stops on a Bomb, the Bomb will explode and remove that Fourzy and any Fourzies in the 8 surrounding spaces from the game."
    },
    {
      "key": "token_desc_cross_bomb",
      "value": "When a Fourzy stops on a Bomb, the Bomb will explode and remove that Fourzy and any Fourzies in th same row and Column as the Bomb."
    },
    {
      "key": "token_desc_darkness",
      "value": "The purpose of the Darkness token will be to hide a square on the board and to prevent a win using anything on that square."
    },
    {
      "key": "token_desc_fourway_arrow",
      "value": "Works in four directions. Sends the Fourzy back where it came from."
    },
    {
      "key": "token_desc_fruit",
      "value": "Fruit are dropped from Fruit Trees. When a Fourzy moves over or stops on Fruit, the Fruit is squished and turns into Goop."
    },
    {
      "key": "token_desc_fruit_tree",
      "value": "Blocker. If a Fourzy is stopped by a Fruit Tree, a Fruit will appear on the opposite side."
    },
    {
      "key": "token_desc_ghost",
      "value": "A Fourzy can pass through a Ghost but cannot stop on a space with a Ghost."
    },
    {
      "key": "token_desc_hex",
      "value": "A Fourzy can move through a Nope but cannot stop on Nope. When a Nope is placed on top of another token, it cancels all effects of that token."
    },
    {
      "key": "token_desc_ice",
      "value": "Fourzies move normally over Ice. Any Fourzy stopped on Ice can be pushed and the new Fourzy will take its place."
    },
    {
      "key": "token_desc_ice_block",
      "value": "Blocker. Crumbles and turns into Ice after blocking a Fourzy."
    },
    {
      "key": "token_desc_lure",
      "value": "When a Fourzy pass in the same column or row as the Lure, the Fourzy will be pulled the same space as the Lure. The Lure will be eaten by the Fourzy."
    },
    {
      "key": "token_desc_move_blocker",
      "value": "Blocker. Move Blockers change their location each turn. Pieces cannot enter or move through this space."
    },
    {
      "key": "token_desc_moving_ghost",
      "value": "This Ghost moves 1 space after each turn."
    },
    {
      "key": "token_desc_pit",
      "value": "A Fourzy stopping on a Pit is removed from the game and the Pit disappears completely."
    },
    {
      "key": "token_desc_rotating_arrow",
      "value": "This Arrow rotates to the right 90 degrees after each turn."
    },
    {
      "key": "token_desc_sand",
      "value": "Sand creates friction. A Fourzy will stop on the 2nd sand it enters. Another Fourzy can push a piece out of Sand and take its place."
    },
    {
      "key": "token_desc_snow",
      "value": "Snow creates friction. A Fourzy will stop on the 2nd Snow it enters. When a Fourzy moves over Snow, the Snow turns into Ice"
    },
    {
      "key": "token_desc_sticky",
      "value": "A Fourzy will stop when entering Goop. A Fourzy already stuck in Goop can be pushed off by another Fourzy."
    },
    {
      "key": "token_desc_water",
      "value": "A Fourzy entering Water will immediately stop. A Fourzy can be pushed out of Water but will only move 1 space."
    },
